{
	"name" : "valu/valufilestorage",
	"description" : "File storage module for ZF2/Valu",
	"type" : "library",
	"keywords" : [
		"valu"
	],
	"homepage" : "http://www.mediacabinet.fi/",
	"license" : [
		"BSD-3-Clause"
	],
	"require" : {
		"php" : ">=5.4.0",
		"zendframework/zendframework" : ">=2.0",
        "valu/valucore" : "dev-master",
        "valu/valuso" : "dev-master",
        "valu/valusetup" : "dev-master",
        "doctrine/mongodb-odm" : "1.0.*",
        "doctrine/doctrine-mongo-odm-module": "0.9.1",
        "doctrine/doctrine-module": "0.8.*"
	},
    "repositories" : [
        {
            "type": "vcs",
            "url":  "https://github.com/valu-digital/valucore.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/valu-digital/valuso.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/valu-digital/valusetup.git"
        }
    ],
	"autoload" : {
		"psr-0" : {
			"ValuFileStorage\\" : "src/"
		}
	},
	"autoload-dev" : {
        "psr-0" : {
            "ValuFileStorageTest\\" : "tests/"
        }
    }
}