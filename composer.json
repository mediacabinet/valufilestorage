{
	"name" : "valu/valufilestorage",
	"description" : "File storage module for ZF2/Valu",
	"version" : "0.9.0",
	"type" : "library",
	"keywords" : [
		"valu"
	],
	"homepage" : "http://www.mediacabinet.fi/",
	"license" : [
		"BSD-3-Clause"
	],
	"require" : {
		"php" : ">=5.4.0",
		"zendframework/zendframework" : ">=2.0"
	},
	"require-dev" : {
		"valu/valucore" : "dev-master",
		"valu/valuso" : "dev-master",
		"valu/valusetup" : "dev-master",
		"doctrine/mongodb-odm" : "1.0.0-BETA9",
		"doctrine/doctrine-mongo-odm-module" : "dev-master",
		"doctrine/doctrine-mongo-odm-module" : "dev-master",
        "doctrine/doctrine-module" : "dev-master as 0.8.x-dev"
	},
	"autoload" : {
		"psr-0" : {
			"ValuFileStorage\\" : "src/"
		}
	},
	"autoload-dev" : {
        "psr-0" : {
            "ValuFileStorageTest\\" : "tests/"
        }
    }
}